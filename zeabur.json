{
  "name": "enhanced-video-audio-api",
  "services": [
    {
      "name": "enhanced-video-audio-api",
      "buildCommand": "pip install -r requirements.txt",
      "startCommand": "uvicorn main:app --host 0.0.0.0 --port $PORT",
      "environment": {
        "PORT": "8000",
        "PYTHONUNBUFFERED": "1",
        "PYTHONIOENCODING": "utf-8"
      }
    }
  ]
}
